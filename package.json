{
  "name": "robbertdewilde",
  "version": "1.0.0",
  "description": "cool personal website",
  "main": "dist/index.html",
  "scripts": {
    "build": "tsc src/index.ts --watch --outDir dist && npm run build:cp-public",
    "build:cp-public": "xcopy public dist /Y",
    "type": "tsc --noEmit",
    "start": "npm run build && npm run open",
    "open": "http-server dist -a localhost -p 1234 -o",
    "watch": "watch 'npm run build' dist"
  },
  "author": "RedundantPancake",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^6.4.0",
    "http-server": "^14.0.0",
    "tsc": "^2.0.3",
    "typescript": "^4.5.2"
  },
  "dependencies": {}
}
